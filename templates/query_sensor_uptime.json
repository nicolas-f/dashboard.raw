{
  "aggs": {
    "daily": {
      "date_histogram": {
        "field": "timestamp",
        "interval": "6h",
        "format": "yyyy-MM-dd HH"
      }
    }
  },
  "size": 0,
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "timestamp": {
              "gte": {{ start_time }},
              "lt": {{ end_time }}
            }
          }
        },
        {
          "match": {
            "producerID": "{{ sensor_id }}"
          }
        }
      ]
    }
  }
}